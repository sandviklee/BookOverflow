{
  "name": "bookimdb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "css-build": "node-sass --omit-source-map-url sass/mystyles.scss css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "test": "jest --watchAll --verbose",
    "typesense": "docker run -i -p 8108:8108 -v/tmp/typesense-server-data/:/data typesense/typesense:0.24.0 --data-dir /data --api-key=gruppe29apikey --listen-port 8108 --enable-cors",
    "typesense-init": "node ./src/init_typesense.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "browserify": "^17.0.0",
    "bulma": "^0.9.4",
    "firebase": "^9.17.1",
    "imgurify": "^2.0.1",
    "install": "^0.13.0",
    "npm": "^9.5.1",
    "pinia": "^2.0.33",
    "pinia-plugin-persistedstate": "^3.1.0",
    "primeicons": "^6.0.1",
    "require-vuejs": "^1.1.3",
    "typesense": "^1.5.3",
    "typesense-instantsearch-adapter": "^2.6.0",
    "vue": "^3.2.45",
    "vue-instantsearch": "^4.8.7",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@types/jest": "^29.4.1",
    "@tresjs/core": "^1.8.1",
    "@types/three": "^0.149.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.1",
    "jest": "^29.5.0",
    "node-sass": "^8.0.0",
    "three": "^0.150.1",
    "vite": "^4.0.0"
  }
